server {
    listen 80;
    server_name opns.tronnet.net;
    rewrite ^(.*) https://$host$1 permanent;
}
 
server {
    listen 443 ssl http2;
    server_name opns.tronnet.net;
    include /etc/nginx/snippets/ssl-params.conf;
    location / {
        proxy_pass https://10.0.0.1:10443;
        include /etc/nginx/snippets/proxy-params.conf;
    }
}
